{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["ES2015"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "include": ["src"]
}
